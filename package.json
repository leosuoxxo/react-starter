{
  "name": "react-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "g": "babel-node command/index.js",
    "start": "cross-env react-app-rewired start",
    "build": "cross-env NODE_ENV=production react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "lint": "eslint \"./src/**/*.{js,jsx}\" && stylelint \"./src/**/*.scss\" --syntax scss",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "node-sass": "^7.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "redux": "^4.0.4",
    "sass-resources-loader": "^2.0.1",
    "enquirer": "^2.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/node": "^7.8.4",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-docs": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addon-storyshots": "^5.3.3",
    "@storybook/addon-viewport": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "@testing-library/react": "^9.4.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^6.0.3",
    "customize-cra": "^0.9.1",
    "eslint": "^6.7.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.1.0",
    "jest-dom": "^4.0.0",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1",
    "react-app-rewire-scss": "^1.0.2",
    "react-app-rewired": "^2.1.5",
    "react-test-renderer": "^16.12.0",
    "redux-logger": "^3.0.6",
    "storybook-dark-mode": "^0.1.9",
    "storybook-react-router": "^1.0.8",
    "stylelint": "^12.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-order": "^3.1.1",
    "stylelint-scss": "^3.13.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js"
    ],
    "coverageReporters": [
      "text"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(scss|css)": [
      "stylelint --fix",
      "git add"
    ],
    "*.+(js|jsx)": [
      "eslint --fix",
      "git add"
    ],
    "*.+(json|css|md)": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
